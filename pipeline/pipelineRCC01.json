{
	"name": "pipelineRCC01",
	"properties": {
		"description": "pipeline de carga rcc txt a synapse analytics",
		"activities": [
			{
				"name": "FlujoRccIden",
				"type": "ExecuteDataFlow",
				"dependsOn": [],
				"policy": {
					"timeout": "1.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "FlujoRccIden",
						"type": "DataFlowReference",
						"parameters": {
							"nomArchRccIden": {
								"value": "'@{pipeline().parameters.par1}'",
								"type": "Expression"
							}
						}
					},
					"staging": {
						"linkedService": {
							"referenceName": "AzureBlobStorage1",
							"type": "LinkedServiceReference"
						},
						"folderPath": "stg01"
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					},
					"traceLevel": "Fine"
				}
			}
		],
		"parameters": {
			"par1": {
				"type": "string",
				"defaultValue": "rcciden_202001.txt"
			}
		},
		"annotations": []
	}
}